{
    "name": "nodejs-training",
    "version": "1.0.0",
    "description": "NodeJS Training",
    "main": "index.js",
    "scripts": {
        "tsc": "tsc",
        "start": "nodemon ./src/index.ts",
        "clean": "rimraf coverage build tmp",
        "lint": "eslint . --ext .ts,.tsx",
        "test": "jest --coverage --verbose --runInBand",
        "test:watch": "jest --watch",
        "prebuild": "npm run lint",
        "build": "tsc -p tsconfig.release.json",
        "build:watch": "tsc -w -p tsconfig.release.json",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/isaiasT/nodejs-training.git"
    },
    "author": "Isaias Tortosa",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/isaiasT/nodejs-training/issues"
    },
    "homepage": "https://github.com/isaiasT/nodejs-training#readme",
    "dependencies": {
        "@types/express": "^4.17.13",
        "body-parser": "^1.19.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-validator": "^6.14.0",
        "lodash": "^4.17.21",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "rollbar": "^2.24.0",
        "sql.js": "^1.6.2",
        "supertest": "^6.2.2",
        "tslib": "^2.3.1",
        "typeorm": "^0.2.44"
    },
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@types/lodash": "^4.14.179",
        "@types/node": "^17.0.20",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-jest": "^26.1.1",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "tsutils": "^3.21.0",
        "typescript": "^4.5.5"
    }
}
